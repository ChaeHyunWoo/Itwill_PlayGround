package com.exe.springdi2;

public class MessageMain {

	public static void main(String[] args) {
		
		System.out.println("1.일반적인 객체생성..");
		MessageKr ob1 = new MessageKr();
		ob1.sayHello("유인나");
		
		MessageEn ob2 = new MessageEn();
		ob2.sayHello("suzi");
		
		
		System.out.println("2.인터페이스 객체생성...");
		
		Message ms = null;
		
		ms = new MessageEn();
		ms.sayHello("insung");
		
	}

}
